{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col">
            {{ form_start(form) }}
            {{ form_errors(form) }}
            {{ form_row(form.email) }}
            {{ form_row(form.pseudo) }}
            {{ form_row(form.plainPassword) }}
            <button type="submit">S'inscrire</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}